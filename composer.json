{
  "name": "host138/php8-smpp",
  "description": "PHP SMPP client lib, based on fork of onlinecity/php-smpp",
  "keywords": [
    "smpp",
    "sms",
    "texting",
    "gsm"
  ],
  "homepage": "https://github.com/host138/php8-smpp",
  "type": "library",
  "license": "LGPL-2.0-or-later",
  "authors": [
    {
      "name": "JS",
      "email": "host138@gmail.com"
    }
  ],
  "require": {
    "php": "8.*",
    "ext-sockets": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpstan/phpstan": "1.4.8"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/host138/php8-smpp.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "smpp\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}